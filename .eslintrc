{
  "env": {
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-var": "error",
    "semi": "error",
    "indent": "error",
    "no-multi-spaces": "error",
    "space-in-parens": "error",
    "no-multiple-empty-lines": "error",
    "prefer-const": "error",
    "no-use-before-define": "error"
  },
  "extends": [
    "plugin:prettier/recommended"
  ]    
}
